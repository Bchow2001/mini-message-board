<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<link rel="stylesheet" href="/stylesheets/style.css" />
	</head>
	<body>
		<h1><%= title %></h1>
		<p>Welcome to <%= title %></p>
		<a href="/new">New Message</a>
		<ul>
			<% messages.forEach(function(message) { %>
			<li>
				<div>Name:<%= message.user %></div>
				<div>Text:<%= message.text %></div>
				<div>Date:<%= message.added%></div>
			</li>
			<% }); %>
		</ul>
	</body>
</html>
